import java.util.ArrayList;
import java.util.List;
public class FamilyNames {
	public static void main(String[] args) {
        List<String> names = new ArrayList<String>();
        names.add("Joasia");//add one element
        names.add("Tomek");//add one element
        names.add("Ania");//add one element
        names.add("Ola");//add one element
        names.add("Keja");//add one element
        names.add("Gala");//add one element

        List<String> names2 = new ArrayList<String>();
        names2.addAll(names);//add all elemnts from list names
        names2.add("Adam");

        System.out.println(names);
        System.out.println(names2);
        
        names2.add("Obcy");
        System.out.println(names2);
        names2.remove("Obcy");//remove Obcy
        System.out.println(names2);
        
        
    }	

}

